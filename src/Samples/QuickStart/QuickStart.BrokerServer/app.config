<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <add key="messageLogFile" value="/home/admin/logs/equeue/messages.log"/>
    <add key="persistMode" value="in-memory"/>
    <add key="persistMessageInterval" value="1000"/>
    <add key="persistMessageMaxCount" value="5000"/>
    <add key="maxCacheMessageSize" value="200000"/>
    <add key="connectionString" value="Data Source=(localdb)\Projects;Integrated Security=true;Initial Catalog=EQueue;Connect Timeout=30;Min Pool Size=10;Max Pool Size=100"/>
  </appSettings>
</configuration>
